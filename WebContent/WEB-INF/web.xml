			<param-name>contextConfigLocation</param-name>
			<param-value>/WEB-INF/salary-servlet.xml</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>dispatcherServlet</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet>
	  <description>Download ReportFile</description>
	  <display-name>DownloadReportFile</display-name>
